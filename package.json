{
  "name": "my-vite-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "commit": "cz",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@reduxjs/toolkit": "^2.6.1",
    "@sendgrid/mail": "^8.1.5",
    "antd": "^5.24.6",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.485.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-a11y": "^9.0.10",
    "@storybook/addon-docs": "^9.0.10",
    "@storybook/addon-onboarding": "^9.0.10",
    "@storybook/addon-vitest": "^9.0.10",
    "@storybook/react-vite": "^9.0.10",
    "@types/i18next": "^12.1.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/browser": "^3.2.3",
    "@vitest/coverage-v8": "^3.2.3",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.10",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "i18next": "^25.1.3",
    "lint-staged": "^16.1.0",
    "playwright": "^1.53.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "react-i18next": "^15.5.1",
    "storybook": "^9.0.10",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
